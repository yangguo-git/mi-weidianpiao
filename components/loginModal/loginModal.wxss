.main{
  width:100%;
  font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
}
.top{
  display:flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin:133rpx auto 66rpx;
}
.t-title{
	font-size: 42rpx;
	color: #333435;
  margin:0rpx auto 26rpx;
}
.t-subTitle{
  font-size: 30rpx;
	color: #72767a;
}
.middle{
  display:flex;
  flex-direction: column;
  padding:27rpx 55rpx;
  border-top:4rpx solid #e5e5e5;
}
.m-title{
  font-size:36rpx;
  color:#333435;
  margin:26rpx 0rpx;
}
.m-subTitle{
  font-size: 36rpx;
	color: #72767a;
  padding-left:27rpx;
}
.bottom{
  display:flex;
  flex-direction: row;
  justify-content:space-evenly;
  margin:50rpx 55rpx auto;
}
.bottom-audit{
  display:flex;
  flex-direction: row;
  justify-content:center;
  margin:50rpx 55rpx auto;
}
.b-login{
  margin:0;
  width: 223rpx;
  text-align: center;
	height: 70rpx;
	background-color: #0ba409;
	border-radius: 10rpx;
  font-size: 28rpx;
	line-height: 70rpx;
	letter-spacing: 2rpx;
	color: #ffffff;
}
.b-login-audit{
  margin:0;
  text-align: center;
  width: 396rpx;
	height: 70rpx;
	background-color: #0ba409;
	border-radius: 10rpx;
  font-size: 28rpx;
	line-height: 70rpx;
	letter-spacing: 2rpx;
	color: #ffffff;
}
.b-refuse{
  width: 223rpx;
  text-align: center;
	height: 70rpx;
	background-color: #fff;
	border-radius: 10rpx;
  font-size: 28rpx;
	line-height: 70rpx;
	letter-spacing: 2rpx;
	color: #828282;
  margin:0;
}