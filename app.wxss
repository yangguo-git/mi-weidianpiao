.flex{
  display: flex;
}
.handup-button{
  width: 690rpx;
  height: 80rpx;
  background: #07af12;
  color: #fff;
  margin-top: 30rpx;
  line-height: 80rpx;
  margin-bottom: 30rpx;
}
.contain{
  background: #fff;

}
.img {
  width: 100%;
  display: block;
}
.nodata{
  text-align: center;
  margin-top: 30rpx;
  color: #999;
  font-size: 32rpx;
}

.invoice-shopname{
  font-size: 36rpx;
  color: #000;
  width: 540rpx;
  /* padding-left: 32rpx; */
  /* line-height: 60rpx; */
  margin: 0 auto;
  
  text-align: center;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.img-logo{
  width: 180rpx;
  display: block;
  margin: 0 auto;
  margin-bottom: 18rpx;
  height: auto;
  /* margin-top: 32rpx; */
}
.main-layout{
  min-height: 100vh;
  box-sizing: border-box;
  display: flex;
  flex: auto;
  flex-direction: column;
}
.main-zoom{
  flex: auto;
}

